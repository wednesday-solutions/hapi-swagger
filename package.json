{
  "name": "hapi-swaggerui",
  "description": "A swagger documentation UI generator plugin for hapi",
  "version": "13.0.1",
  "author": "Glenn Jones",
  "repository": {
    "type": "git",
    "url": "git://github.com/glennjones/hapi-swaggerui.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib",
    "public",
    "templates",
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "api",
    "docs",
    "swagger",
    "hapi",
    "joi"
  ],
  "scripts": {
    "start": "node examples/simple.js",
    "start:basic": "node examples/basic.js",
    "start:debug": "node examples/debug.js",
    "start:jwt": "node examples/jwt.js",
    "start:dot:grouping": "node examples/dot-grouping.js",
    "start:extend": "node examples/extend.js",
    "start:group:ordered": "node examples/group-ordered.js",
    "start:routes:simple": "node examples/group-ordered.js",
    "test": "npm run test:lib",
    "test:lib": "lab -L -t 98 -I 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__,CSS'",
    "test:ts": "tsd",
    "lint": "eslint .",
    "test:cov:html": "lab -r html -o coverage.html",
    "test:cov:coveralls": "npx lab -r lcov | npx coveralls",
    "precommit": "npm test",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hoek": "^9.2.0",
    "handlebars": "^4.5.3",
    "http-status": "^1.0.1",
    "joi": "^17.4.0",
    "json-schema-ref-parser": "^9.0.7",
    "swagger-parser": "10.0.2",
    "swagger-ui-dist": "^3.22.1"
  },
  "devDependencies": {
    "@hapi/basic": "^6.0.0",
    "@hapi/code": "^8.0.3",
    "@hapi/good": "^9.0.1",
    "@hapi/good-console": "^9.0.1",
    "@hapi/h2o2": "^9.0.2",
    "@hapi/hapi": "^20.1.2",
    "@hapi/inert": "5.2.2",
    "@hapi/lab": "^24.1.1",
    "@hapi/vision": "5.5.4",
    "@hapi/wreck": "^17.1.0",
    "@types/hapi__hapi": "^20.0.7",
    "babel-eslint": "^10.0.3",
    "blipp": "^4.0.0",
    "chalk": "^4.1.1",
    "coveralls": "^3.0.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^18.0.0",
    "hapi-api-version": "^2.1.0",
    "hapi-auth-bearer-token": "^8.0.0",
    "hapi-auth-jwt2": "^10.2.0",
    "husky": "^6.0.0",
    "js2xmlparser": "^4.0.0",
    "jsonwebtoken": "^8.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "swagger-client": "^3.9.4",
    "tsd": "^0.14.0"
  },
  "peerDependencies": {
    "@hapi/hapi": "^18.0.0"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "npm test",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
